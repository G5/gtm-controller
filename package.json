{
  "name": "@g5/gtm-controller",
  "version": "1.0.0",
  "description": "Modular GTM scripts with unit tests, modern JavaScript, backed by the power of Git.",
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "cross-env JEST_COVERAGE=true jest",
    "test:watch": "jest --watch",
    "test:watch:coverage": "cross-env JEST_COVERAGE=true jest --watch",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/G5/gtm-controller.git"
  },
  "keywords": [
    "GTM",
    "Google",
    "Tag",
    "Manager",
    "Script",
    "Manager"
  ],
  "author": "G5 <https://github.com/g5/>, Cody Swartz <https://github.com/ctsstc>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/G5/gtm-controller/issues"
  },
  "homepage": "https://github.com/G5/gtm-controller#readme",
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/jest": "^24.0.0",
    "@types/lodash.escaperegexp": "^4.1.4",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "jack-of-all-decorators": "^1.1.0-beta.1",
    "lodash.escaperegexp": "^4.1.2",
    "snyk": "^1.316.1"
  },
  "snyk": true
}
